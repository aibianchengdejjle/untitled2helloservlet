<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>file</servlet-name>
        <servlet-class>com.jjl.servlet.fileservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>file</servlet-name>
        <url-pattern>/file</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>image</servlet-name>
        <servlet-class>com.jjl.servlet.imageservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>image</servlet-name>
        <url-pattern>/image</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>redire</servlet-name>
        <servlet-class>com.jjl.servlet.Redire</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>redire</servlet-name>
        <url-pattern>/redire</url-pattern>
    </servlet-mapping>
</web-app>